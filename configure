typedef unsigned int	__u64;
#else
#if ($ocfs2_SIZEOF_LONG == 8)
typedef long		__s64;
typedef unsigned long	__u64;
#else
#if ($ocfs2_SIZEOF_LONG_LONG == 8)
#if defined(__GNUC__)
typedef __signed__ long long 	__s64;
#else
